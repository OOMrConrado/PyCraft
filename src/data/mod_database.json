  "error_patterns": {
    "_comment": "Known error patterns and how to handle them",
    "patterns": [
      {
        "id": "client_class_dedicated_server",
        "regex": "Attempted to load class.*for invalid dist DEDICATED_SERVER",
        "description": "Client-only class loaded on dedicated server",
        "action": "identify_and_remove",
        "extract_mod_from": "mixin_config"
      },
      {
        "id": "mixin_client_target_not_found",
        "regex": "@Mixin target.*client.*was not found",
        "description": "Mixin targeting client class on server",
        "action": "identify_and_remove",
        "extract_mod_from": "mixin_config"
      },
      {
        "id": "missing_dependency",
        "regex": "Mod.*requires.*is not installed|requires.*not installed|Unsatisfied dependency",
        "description": "Missing mod dependency",
        "action": "install_or_remove",
        "extract_mod_from": "error_message"
      },
      {
        "id": "fabric_environment_mismatch",
        "regex": "environment.*CLIENT.*DEDICATED_SERVER|cannot load.*client.*on.*server",
        "description": "Fabric mod in wrong environment",
        "action": "identify_and_remove",
        "extract_mod_from": "mod_id"
      },
      {
        "id": "class_not_found_client",
        "regex": "NoClassDefFoundError.*(net/minecraft/client/|com/mojang/blaze3d/)",
        "description": "Client class not found on server",
        "action": "identify_and_remove",
        "extract_mod_from": "stacktrace"
      },
      {
        "id": "mixin_apply_error",
        "regex": "Mixin apply.*failed|MixinApplyError",
        "description": "Mixin failed to apply",
        "action": "identify_and_remove",
        "extract_mod_from": "mixin_config"
      },
      {
        "id": "mod_loading_exception",
        "regex": "ModLoadingException|LoadingFailedException",
        "description": "General mod loading error",
        "action": "identify_and_remove",
        "extract_mod_from": "error_message"
      }
    ]
  },

  "fatal_errors": {
    "_comment": "Errors that cannot be auto-fixed and require user intervention",
    "patterns": [
      {
        "id": "out_of_memory",
        "regex": "OutOfMemoryError|GC overhead limit exceeded|Java heap space",
        "description": "Server ran out of memory",
        "user_message": "The server ran out of memory. Go to Settings and increase the allocated RAM."
      },
      {
        "id": "port_in_use",
        "regex": "Address already in use|EADDRINUSE|Failed to bind to port",
        "description": "Port 25565 is already in use",
        "user_message": "Port 25565 is already in use. Close any other Minecraft server or change the port in server.properties."
      },
      {
        "id": "java_version_mismatch",
        "regex": "UnsupportedClassVersionError|class file version.*current.*version",
        "description": "Wrong Java version",
        "user_message": "Incorrect Java version. Go to Settings > Java to install the correct version."
      },
      {
        "id": "corrupted_world",
        "regex": "Exception reading.*level\\.dat|Corrupted.*region file|InvalidNBTException",
        "description": "World data is corrupted",
        "user_message": "World data appears to be corrupted. Try restoring a backup or deleting the world/ folder."
      },
      {
        "id": "duplicate_mod",
        "regex": "Duplicate mod|Found duplicate|already loaded.*same id",
        "description": "Duplicate mod detected",
        "user_message": "There are duplicate mods. Check the mods/ folder and remove duplicate copies."
      }
    ]
  },

  "modrinth_cache": {
    "_comment": "Cache of mod info from Modrinth API - populated at runtime",
